  "UseCors": true,
  "UseDefaultCors": true,
  "CorsOptions": [
    {
      "PolicyName": "SystemCors",
      "AllowAnyOrigins": true,
      "WithOrigins": [],
      "AllowAnyMethods": true,
      "WithMethods": [],
      "AllowAnyHeaders": true,
      "WithHeaders": []
    },
    {
      "PolicyName": "Default",
      "AllowAnyOrigins": true,
      "WithOrigins": [],
      "AllowAnyMethods": true,
      "WithMethods": [],
      "AllowAnyHeaders": true,
      "WithHeaders": []
    }
  ],
  "LoadControllersAeesmblies": [
    "WWC.240711.ASPNETCore.Extensions",
    "WWC.240711.ASPNETCore.Production"
  ]
